<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Hibernate.Cliente">
        <table name="cliente" schema="ventas" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="nombre">
                <column name="nombre"/>
            </basic>
            <basic name="direccion">
                <column name="direccion"/>
            </basic>
            <basic name="poblacion">
                <column name="poblacion"/>
            </basic>
            <basic name="telef">
                <column name="telef"/>
            </basic>
            <basic name="nif">
                <column name="nif"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
