<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Hibernate.Producto">
        <table name="producto" schema="ventas" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="descripcion">
                <column name="descripcion"/>
            </basic>
            <basic name="stockactual">
                <column name="stockactual"/>
            </basic>
            <basic name="stockminimo">
                <column name="stockminimo"/>
            </basic>
            <basic name="precio">
                <column name="precio"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
