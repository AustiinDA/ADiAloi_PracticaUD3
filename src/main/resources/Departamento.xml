<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Hibernate.Departamento">
        <table name="departamento" schema="personal" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="idDep">
                <column name="id_dep"/>
            </basic>
            <basic name="nombre">
                <column name="nombre"/>
            </basic>
            <basic name="localidad">
                <column name="localidad"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
