<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Venta">
        <table name="venta" schema="ventas" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="fecha">
                <column name="fecha"/>
            </basic>
            <basic name="cantidad">
                <column name="cantidad"/>
            </basic>
            <basic name="idCliente">
                <column name="id_cliente"/>
            </basic>
            <basic name="idProducto">
                <column name="id_producto"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
