<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Hibernate.Empleado">
        <table name="empleado" schema="personal" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="idEmp">
                <column name="id_emp"/>
            </basic>
            <basic name="apellido">
                <column name="apellido"/>
            </basic>
            <basic name="oficio">
                <column name="oficio"/>
            </basic>
            <basic name="fechaAlta">
                <column name="fecha_alta"/>
            </basic>
            <basic name="salario">
                <column name="salario"/>
            </basic>
            <basic name="comision">
                <column name="comision"/>
            </basic>
            <basic name="idDep">
                <column name="id_dep"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
